                            <input class="field__input required numberOnly"
                                   id="@data.fields[15].id"
                                   name="@data.fields[15].id"
                                   type="text"
                                   maxlength="2" />
                        </div>
                    </div>
                </div>

                <!-- Note -->
                <p class="note">
                    @Html.Raw(data.fields[16].note)
                </p>
            </div>

            <!-- ******* ASIDE  *******-->
