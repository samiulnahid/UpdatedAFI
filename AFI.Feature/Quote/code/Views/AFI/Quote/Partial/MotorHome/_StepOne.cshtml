                        <div class="field__label-wrapper">
                            <label class="field__label" for="additionalDriver0MaritalStatus">@datavalue.fields[4].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input required"
                                    id="@datavalue.fields[4].id"
                                    name="@datavalue.fields[4].id"
                                    type="select"
                                    required="" data-field="MaritalStatus">
                                <option value="">Choose One</option>
                                @foreach (Options opt in datavalue.fields[4].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                        </div>
                    </div>
                    <div class="field field--extended-label">
                        <div class="field__label-wrapper">
