@model AFI.Feature.QuoteForm.Areas.AFIQuoteForm.Models.Motorhome.MotorhomeForm
@using AFI.Feature.QuoteForm.Areas.AFIWEB.Models.Form_Models

@{
    int i = 1;
    var stepsCount = Model.wayfinder.steps.Count;
    int j = 1;
    int n = 1;
    int m = 1;
}

<!-- ************************************************ -->
<!---------------------- MULTI-STEP 1 ------------------>
<!-- ************************************************ -->
<!--Policy Holder-->
@{
    Section data = Model.form.sections.Where(x => x.id == "policyHolder").SingleOrDefault();
}
<section class="form_section multi__step_1 policyholder_form">
    <div class="container">
        <!-- ******** STEP ******** -->
        <div class="step__container">
            <ul class="steps__list">
                @foreach (var step in Model.wayfinder.steps)
                {
                    <li class="step @(i == 1 ? "active_step" : "")" data-text="@step.label">@i</li>
                    @Html.Raw(i != stepsCount ? "<li class='line'></li>" : "")
                    i++;
                }
            </ul>
        </div>

        <div class="quote_request__section_wrapper">
            <!-- ******* FORM  *******-->
            <div class="form_container">
                <!--Updated By  25.7.23 Start-->
                <button class="quote-request-sidebar__show-trigger" type="button">?</button>
                <!--Updated By  25.7.23 End-->
                <h2 class="quote_request_heading">@data.heading</h2>

                <!-- First Name, Last Name, Suffix -->
                <div class="fields-row">
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderFirstName">@data.fields[0].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required alphabeticOnly"
                                   id="@data.fields[0].id"
                                   name="@data.fields[0].id"
                                   type="text"
                                   pattern="^[a-zA-Z]+$"
                                   maxlength="50"
                                   placeholder=""
                                   value="" />
                        </div>
                    </div>
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderLastName">@data.fields[1].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required alphabeticOnly"
                                   id="@data.fields[1].id"
                                   name="@data.fields[1].id"
                                   type="text"
                                   pattern="^[a-zA-Z]+$"
                                   maxlength="50"
                                   placeholder=""
                                   value="" />
                        </div>
                    </div>
                    <div class="field field--small">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderSuffix">@data.fields[2].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input" id="@data.fields[2].id" name="@data.fields[2].id" type="select">
                                <option value="">--</option>
                                @foreach (Options opt in data.fields[2].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                            <p class="field__note">@data.fields[2].note</p>
                        </div>
                    </div>
                </div>

                <!-- Physical Address, City -->
                <div class="fields-row">
                    <div class="field field--xlarge extra_message_margin">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderMailingAddress"> @data.fields[3].label </label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required"
                                   id="@data.fields[3].id"
                                   name="@data.fields[3].id"
                                   type="text"
                                   maxlength="100"
                                   placeholder=""
                                   autocomplete="none"
                                   value="" />
                            <p class="field__note">@data.fields[3].note</p>
                        </div>
                    </div>
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderCity">@data.fields[4].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required"
                                   id="@data.fields[4].id"
                                   name="@data.fields[4].id"
                                   type="text"
                                   maxlength="50"
                                   placeholder=""
                                   autocomplete="none"
                                   value="" />
                        </div>
                    </div>
                </div>

                <!-- State, Zip Code -->
                <div class="fields-row">
                    <div class="field field--xlarge">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderState">@data.fields[5].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input required" id="@data.fields[5].id" name="@data.fields[5].id" type="select">
                            </select>
                        </div>
                    </div>
                    <div class="field field--medium">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderZip">@data.fields[6].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input zip required numberOnly"
                                   id="@data.fields[6].id"
                                   name="@data.fields[6].id"
                                   type="text"
                                   pattern="[0-9]{5}"
                                   maxlength="5"
                                   placeholder=""
                                   autocomplete="none"
                                   value="" />
                        </div>
                    </div>
                </div>

                <!-- Social Security Number,  Date of Birth, Operating Experience (Years) -->
                <div class="fields-row">
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderSsn">@data.fields[7].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input SSN"
                                   id="@data.fields[7].id"
                                   name="@data.fields[7].id"
                                   type="text"
                                   maxlength="11"
                                   placeholder="@data.fields[7].placeholder"
                                   value="" />
                            <p class="field__note">@data.fields[7].note</p>
                        </div>
                    </div>
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderDob">@data.fields[8].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required date checkAdult"
                                   id="@data.fields[8].id"
                                   name="@data.fields[8].id"
                                   type="text"
                                   pattern="\d{1,2}/\d{1,2}/\d{4}"
                                   placeholder="@data.fields[8].placeholder"
                                   value="" />
                        </div>
                    </div>

                    <div class="field field--small field--extended-label">
                        <div class="field__label-wrapper">
                            <label class="field__label motor-experience" for="policyHolderAgeWhenLicensed">@data.fields[9].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <!--Updated By  25.7.23 Start-->
                            <input class="field__input required numberOnly"
                                   id="@data.fields[9].id"
                                   name="@data.fields[9].id"
                                   type="text"
                                   maxlength="2" />
                            <!--Updated By  25.7.23 End-->
                        </div>
                    </div>
                </div>

                <!-- Gender,  Marital Status-->
                <div class="fields-row">
                    <div class="field field--large">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderGender">@data.fields[10].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input required" id="@data.fields[10].id" name="@data.fields[10].id" type="select">
                                <option value="">--</option>
                                @foreach (Options opt in data.fields[10].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                        </div>
                    </div>
                    <div class="field field--large">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderMaritalStatus">@data.fields[11].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input required"
                                    id="@data.fields[11].id"
                                    name="@data.fields[11].id"
                                    type="select">
                                <option value="">--</option>
                                @foreach (Options opt in data.fields[11].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                        </div>
                    </div>
                </div>

                <!-- Email Address, Phone Type, Phone Number-->
                <div class="fields-row">
                    <div class="field field--xlarge">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderEmail">@data.fields[12].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input required email"
                                   id="@data.fields[12].id"
                                   name="@data.fields[12].id"
                                   type="email"
                                   placeholder=""
                                   value="" />
                        </div>
                    </div>
                    <div class="field field--medium">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderPhoneType">@data.fields[13].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input required" id="@data.fields[13].id" name="@data.fields[13].id" type="select">
                                <option value="">--</option>
                                @foreach (Options opt in data.fields[13].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                        </div>
                    </div>
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderPhoneNumber">@data.fields[14].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <input class="field__input phone required"
                                   id="@data.fields[14].id"
                                   name="@data.fields[14].id"
                                   type="text"
                                   pattern="\(\d\d\d\) \d\d\d-?\d\d\d\d"
                                   placeholder=""
                                   value="" />
                        </div>
                    </div>
                </div>

                <!-- Residence Status -->
                <div class="fields-row">
                    <div class="field">
                        <div class="field__label-wrapper">
                            <label class="field__label" for="policyHolderResidenceStatus">@data.fields[15].label</label>
                        </div>
                        <div class="field__input-wrapper">
                            <select class="field__input "
                                    id="@data.fields[15].id"
                                    name=" @data.fields[15].id "
                                    type="select">
                                <option value="">Choose One</option>
                                @foreach (Options opt in data.fields[15].options)
                                {
                                    <option value="@opt.value">@opt.label</option>
                                }
                            </select>
                        </div>
                    </div>
                </div>

                <!-- Note -->
                <p class="note">
                    @Html.Raw(data.fields[16].note)
                </p>
            </div>

            <!-- ******* ASIDE  *******-->
            @if (data.sidebar != null && data.sidebar.content.Count > 0)
            {
        <aside class="quote_request_aside">
            <h2 class="quote_request_sidebar__heading">@data.sidebar.heading</h2>
            <div class="quote_request_sidebar__content">
                @foreach (SideBarContent sideContent in data.sidebar.content)
                {
                    <div class="quote_request_sidebar__item">
                        <h3 class="quote_request_sidebar__item_heading">
                            @sideContent.heading
                        </h3>
                        <p class="quote_request_sidebar__item_description">
                            @sideContent.content
                        </p>
                    </div>
                }

            </div>
            <!--Updated By  25.7.23 Start-->
            <button class="button button--reversed quote-request-sidebar__hide-trigger" type="button">Close</button>
            <!--Updated By 25.7.23 End-->
        </aside>
            }
        </div>
    </div>
</section>

