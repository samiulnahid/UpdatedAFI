@{
    Section datasummary = Model.form.sections.Where(x => x.id == "driverSummary").SingleOrDefault();
}
<!--  ADDITIONAL DRIVER Summary -->
<section class="form_section multi__step_2 driver_summary_form">
    <div class="container">
        <!-- ******** STEP ******** -->
        <div class="step__container">
            <ul class="steps__list">
                @foreach (var step in Model.wayfinder.steps)
                {
                    <li class="step @(m == 1 ? "active_step" : "")" data-text="@step.label">@m</li>
                    @Html.Raw(m != stepsCount ? "<li class='line'></li>" : "")
                    m++;
                }
            </ul>
        </div>

        <div class="quote_request__section_wrapper">
            <div class="form_container">
               <h2 class="quote_request_heading">@datasummary.heading</h2>
                <!-- Summary -->
                <div class="quote_request__summary">
                    <!-- PolicyHoler -->
                    <div class="quote_request__summary_item quote_request__summary_policyholer_item">
                        <p class="quote_request__summary_item_info quote_request__summary_policyholder_item_info"></p>
                        <div class="quote_request__summary_item_actions delete_edit">
                            <button id="policyholderEditBtn" class="button-bare" type="button">Edit</button>
                        </div>
                    </div>

                    <!-- Spouse -->
                    <div class="quote_request__summary_item quote_request__summary_spouse_item __hide">
                        <p class="quote_request__summary_item_info quote_request__summary_spouse_item_info"></p>
                        <div class="quote_request__summary_item_actions delete_edit">
                            <button id="spouseEditBtn" class="button-bare" type="button">Edit</button>
                        </div>
                    </div>

                    <!-- Demo Vehicles Items -->
                    <div class="quote_request__summary_item driverItem driverDemoItem __hide">
                        <p class="quote_request__summary_item_info"></p>
                        <div class="quote_request__summary_item_actions delete_edit">
                            <button id="deleteBtn" class="button-bare deleteBtn" type="button">Delete</button>
                            <button id="editBtn" class="button-bare editBtn" type="button">Edit</button>
                        </div>
                        <div class="quote_request__summary_item_actions yes_no __hide">
                            <!--Updated By  24.7.23 Start-->
                            <span>Are you sure?</span>
                            <!--Updated By  24.7.23 End-->
                            <button id="deleteYes" class="button-bare deleteYes" type="button">Yes</button>
                            <button id="deleteNo" class="button-bare deleteNo" type="button">No</button>
                        </div>
                    </div>
                    <!-- More Vehicles Items -->
                    <div id="MHaddDriversList"></div>
                </div>

                <!-- Add Button -->
                <button id="MHaddDriver" class="button" type="button">@Model.form.unique.addDriverButtonText</button>
            </div>
        </div>
    </div>
</section>
